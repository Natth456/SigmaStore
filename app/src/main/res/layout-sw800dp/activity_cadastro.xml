<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFC107"
    tools:context=".Cadastro"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="10dp">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="159dp"
        android:layout_height="22dp"
        android:layout_marginStart="127dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="354dp"
        android:layout_marginBottom="1dp"
        android:scaleX="2"
        android:scaleY="2"
        android:text="Senha"
        app:layout_constraintBottom_toTopOf="@+id/senha_cadastro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/voltar_cadastro"
        app:layout_constraintTop_toBottomOf="@+id/cpf_cadastro" />

    <Button
        android:id="@+id/cadastrar"
        android:layout_width="572dp"
        android:layout_height="84dp"
        android:layout_marginStart="70dp"
        android:layout_marginTop="64dp"
        android:backgroundTint="#0F0E0E"
        android:text="CADASTRAR"
        app:layout_constraintBottom_toTopOf="@+id/voltar_cadastro"
        app:layout_constraintEnd_toStartOf="@+id/cpf_cadastro"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <Button
        android:id="@+id/voltar_cadastro"
        android:layout_width="570dp"
        android:layout_height="72dp"
        android:layout_marginStart="70dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="108dp"
        android:backgroundTint="#0F0E0E"
        android:text="VOLTAR"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/senha_cadastro"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cadastrar" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="300dp"
        android:layout_height="228dp"
        android:layout_marginStart="110dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="111dp"
        app:layout_constraintBottom_toTopOf="@+id/cadastrar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.115"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sigma_logo" />

    <EditText
        android:id="@+id/nome_cadastro"
        android:layout_width="565dp"
        android:layout_height="80dp"
        android:layout_marginStart="174dp"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="44dp"
        android:ems="10"
        android:inputType="text"
        android:singleLine="true"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="364dp"
        android:layout_height="28dp"
        android:layout_marginStart="362dp"
        android:layout_marginTop="157dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="1dp"
        android:scaleX="2"
        android:scaleY="2"
        android:text="Nome"
        app:layout_constraintBottom_toTopOf="@+id/nome_cadastro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/senha_cadastro"
        android:layout_width="565dp"
        android:layout_height="68dp"
        android:layout_marginStart="671dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="44dp"
        android:ems="10"
        android:inputType="textPassword"
        android:singleLine="true"
        app:layout_constraintBottom_toTopOf="@+id/adm_switch"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <EditText
        android:id="@+id/cpf_cadastro"
        android:layout_width="565dp"
        android:layout_height="75dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="44dp"
        android:ems="10"
        android:inputType="textPassword"
        android:singleLine="true"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cadastrar"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="192dp"
        android:layout_height="30dp"
        android:layout_marginStart="125dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="321dp"
        android:layout_marginBottom="1dp"
        android:scaleX="2"
        android:scaleY="2"
        android:text="CPF"
        app:layout_constraintBottom_toTopOf="@+id/cpf_cadastro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cadastrar"
        app:layout_constraintTop_toBottomOf="@+id/nome_cadastro" />

    <Switch
        android:id="@+id/adm_switch"
        android:layout_width="90dp"
        android:layout_height="61dp"
        android:layout_marginStart="895dp"
        android:layout_marginTop="61dp"
        android:layout_marginEnd="295dp"
        android:layout_marginBottom="113dp"
        android:scaleX="2"
        android:scaleY="2"
        android:text="ADM"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/senha_cadastro" />

</androidx.constraintlayout.widget.ConstraintLayout>